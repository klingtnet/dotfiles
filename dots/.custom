# user specific settings and things

[[ -d "$HOME/.go" ]]\
	&& export GOPATH=${GOPATH:-~/.go}

[[ ! -d $HOME/bin ]]\
    && mkdir $HOME/bin

[[ -f ~/.pyenvrc ]]\
	&& source ~/.pyenvrc

[[ -d "$HOME/.node_modules" ]]\
	&& export npm_config_prefix="$HOME/.node_modules"

[[ -d "$HOME/.vst" ]]\
	&& export VST_PATH=${VST_PATH:-~/.vst}
